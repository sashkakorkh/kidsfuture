{
  "name": "kids-future",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@fontsource/merriweather": "^5.0.8",
    "@fontsource/pt-sans": "^5.0.8",
    "@mui/material": "^5.14.16",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-styles-sass start-js",
    "build": "npm run eslint && npm run stylelint-build &&  npm run test-pre-build && react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "test-pre-build": "npm test -- --watchAll=false",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "watch-styles-sass": "npm-watch",
    "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
    "eslint": "eslint src/**/*.js src/**/*.jsx"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js",
        "/src/**/*.jsx"
      ],
      "extensions": "js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "standard": "^10.0.3"
  }
}
